I use flag for this one....



function truthCheck(collection, pre) {
  // Is everyone being true?
 var a = 0;
  for(var i = 0; i < collection.length; i++){
if(collection[i][pre] !== "" && typeof(collection[i][pre])==="string" ||typeof(collection[i][pre])==="boolean" ){
   a++; 
   }  
  }
  if (a===collection.length){
    return true;
  }
  else{
    return false;
  }

}
